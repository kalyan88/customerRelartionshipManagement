package com.crmshopping.dto;

public class Customerlanguage {

	private String customerId;
	
	private String languageId;
	
	private String customerLanguage;
	
	private String preferenceLevel;

	public String getCustomerId() {
		return customerId;
	}

	public void setCustomerId(String customerId) {
		this.customerId = customerId;
	}

	public String getCustomerLanguage() {
		return customerLanguage;
	}

	public void setCustomerLanguage(String customerLanguage) {
		this.customerLanguage = customerLanguage;
	}

	public String getLanguageId() {
		return languageId;
	}

	public void setLanguageId(String languageId) {
		this.languageId = languageId;
	}

	public String getPreferenceLevel() {
		return preferenceLevel;
	}

	public void setPreferenceLevel(String preferenceLevel) {
		this.preferenceLevel = preferenceLevel;
	}
	
}
